{
  "name": "URL Scissors",
  "version": "3.0.0",
  "manifest_version": 2,
  "description": "URL Cutting Tool",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "URL Scissors",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  }
}
